meta {
  name: 02 - Get Products
  type: graphql
  seq: 11
}

post {
  url: http://localhost:3000/graphql
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{VAR_JWT}}
}

body:graphql {
  query {
    products {
      id
      name
      price
      category {
        id
        name
      }
    }
  }
}

settings {
  encodeUrl: true
  timeout: 0
}
